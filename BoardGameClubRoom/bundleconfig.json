[
  {
    "outputFileName": "wwwroot/bundled/css/lib.min.css",
    "inputFiles": [
      "wwwroot/lib/bootstrap/dist/css/*.css",
      "wwwroot/lib/fontawesome/css/*.css"
    ]
  },
  {
    "outputFileName": "wwwroot/bundled/js/lib.min.js",
    "inputFiles": [
      "wwwroot/lib/jquery/dist/*.js",
      "wwwroot/lib/jquery-validation/dist/jquery.validate.js",
      "wwwroot/lib/jquery-validation/dist/additional-methods.js",
      "wwwroot/lib/jquery-validation-unobtrusive/*.js",
      "wwwroot/lib/bootstrap/dist/js/*.js"
    ],
    "minify": {
      "enabled": false,
      "renameLocals": false
    },
    "sourceMap": true
  },

  
  {
    "outputFileName": "wwwroot/bundled/css/site.css",
    "inputFiles": [
      "wwwroot/css/Base/*.css",
      "wwwroot/lib/bootstrap/dist/css/*.css"
    ]
  },
  {
    "outputFileName": "wwwroot/bundled/js/site.js",
    "inputFiles": [
      "wwwroot/js/Base/*.js"
    ],
    "minify": {
      "enabled": false,
      "renameLocals": false
    },
    "sourceMap": true
  }
]
